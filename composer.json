{
    "name": "jms/twig-js",
    "type": "library",
    "description": "twig.js, flexible, secure, and high-performance templating engine for Javascript",
    "keywords": ["templating"],
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "twig/twig": "^2.11"
    },
    "require-dev": {
        "phpunit/phpunit": "8.3.5",
        "datto/json-rpc-http": "^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "TwigJs\\": "src/TwigJs"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwigJs\\Tests\\": "tests/TwigJs/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
